{
    "name": "brunocfalcao/eduka-pathfinder",
    "description": "Eduka LMS multi-site finder",
    "keywords": ["Eduka", "Tutorials", "Laravel"],
    "homepage": "https://github.com/brunocfalcao/eduka-pathfinder",
    "license": "MIT",
    "authors": [{
        "name": "Bruno Falcao",
        "email": "bruno.c.falcao@brunofalcao.me",
        "homepage": "https://www.brunocfalcao.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.14|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\Pathfinder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\Pathfinder\\EdukaPathfinderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
